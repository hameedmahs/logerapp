<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loger">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".Notify"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/art"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/artr"
        android:supportsRtl="true"
        android:theme="@style/Theme.Loger">
        <activity android:name=".LogVarifyActivity"></activity>
        <activity android:name=".EditImageActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".SyllubusActivity" />
        <activity android:name=".InternalsActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".FeeInfoActivity" />
        <activity android:name=".LiquidActivity" />
        <activity android:name=".splashScreentest">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".services.MyFirebaseInstanceServices">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".HomeActivity" />
        <activity android:name=".VarifyActivity" />
        <activity android:name=".RegisterActivity2" />
        <activity android:name=".RegisterActivity1" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Theme.AppCompat" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>